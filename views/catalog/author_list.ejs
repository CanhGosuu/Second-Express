<!DOCTYPE html>
<html lang="en">

<head>
    <% include ../layouts/head %>
</head>

<body class="container">
    <!-- header -->
    <% include ../layouts/header %>
    <!-- main -->
    <h1>
        <%=title%>
    </h1>
    <ul>
        <%if(author_list){ %>
        <% for(var i=0; i<author_list.length; i++) {%>
        <li> <a href="<%=author_list[i].url%>">
                <%=author_list[i].name%></a>
            <%=author_list[i].date_of_birth_formatted%> -
            <%=author_list[i].date_of_death_formatted%>
        </li>
        <% }} else{ %>
        <li>There are no author_lists.</li>
        <% } %>
    </ul>


    <!-- main -->
    </div>
    </div>
    </div>
    <footer>
        <% include ../layouts/footer %>
    </footer>

</body>